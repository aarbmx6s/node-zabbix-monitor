    <templates>
        <template>
            <template>CentOS 7</template>
            <name>CentOS 7</name>
            <groups>
                <group>
                    <name>CentOS 7</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Centos7</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>CPU: max</name>
                    <type>DEPENDENT</type>
                    <key>system.cpu.max</key>
                    <delay>0</delay>
                    <applications>
                        <application>
                            <name>Centos7</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>100</params>
                        </step>
                    </preprocessing>
                    <master_item>
                        <key>system.cpu.num[max]</key>
                    </master_item>
                </item>
                <item>
                    <name>CentOS CPU: cores</name>
                    <key>system.cpu.num[max]</key>
                    <applications>
                        <application>
                            <name>Centos7</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>CentOS CPU: load</name>
                    <key>system.cpu.util</key>
                    <delay>10s</delay>
                    <value_type>FLOAT</value_type>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>Centos7</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>CentOS DISK: free</name>
                    <key>vfs.fs.size[/,free]</key>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Centos7</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>CentOS DISK: total</name>
                    <key>vfs.fs.size[/,total]</key>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Centos7</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>CentOS DISK: used</name>
                    <key>vfs.fs.size[/,used]</key>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>Centos7</name>
                        </application>
                    </applications>
                </item>
            </items>
        </template>
        <template>
            <template>MongoDB</template>
            <name>MongoDB</name>
            <groups>
                <group>
                    <name>MongoDB</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>MongoDB</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>MongoDB: all</name>
                    <key>nz.collect.mongo[10]</key>
                    <delay>10s</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>MongoDB</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>MongoDB: dstats</name>
                    <type>DEPENDENT</type>
                    <key>nz.collect.mongo.dstats</key>
                    <delay>0</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>MongoDB: dstats: {#HOST}: connections</name>
                            <type>CALCULATED</type>
                            <key>nz.collect.mongo.dhost[{#HOST},conns]</key>
                            <delay>10s</delay>
                            <params>{#CONNECTION}</params>
                            <applications>
                                <application>
                                    <name>MongoDB</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>MongoDB: dstats: {#HOST}: op: delete</name>
                            <type>CALCULATED</type>
                            <key>nz.collect.mongo.dhost[{#HOST},delete]</key>
                            <delay>10s</delay>
                            <params>{#DELETE}</params>
                            <applications>
                                <application>
                                    <name>MongoDB</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>MongoDB: dstats: {#HOST}: op: insert</name>
                            <type>CALCULATED</type>
                            <key>nz.collect.mongo.dhost[{#HOST},insert]</key>
                            <delay>10s</delay>
                            <params>{#INSERT}</params>
                            <applications>
                                <application>
                                    <name>MongoDB</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>MongoDB: dstats: {#HOST}: op: query</name>
                            <type>CALCULATED</type>
                            <key>nz.collect.mongo.dhost[{#HOST},query]</key>
                            <delay>10s</delay>
                            <params>{#QUERY}</params>
                            <applications>
                                <application>
                                    <name>MongoDB</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>MongoDB: dstats: {#HOST}: op: update</name>
                            <type>CALCULATED</type>
                            <key>nz.collect.mongo.dhost[{#HOST},update]</key>
                            <delay>10s</delay>
                            <params>{#UPDATE}</params>
                            <applications>
                                <application>
                                    <name>MongoDB</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>MongoDB: dstats: {#HOST}: connections</name>
                            <ymin_type_1>FIXED</ymin_type_1>
                            <graph_items>
                                <graph_item>
                                    <color>FF4000</color>
                                    <item>
                                        <host>MongoDB</host>
                                        <key>nz.collect.mongo.dhost[{#HOST},conns]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>MongoDB: dstats: {#HOST}: operations</name>
                            <ymin_type_1>FIXED</ymin_type_1>
                            <graph_items>
                                <graph_item>
                                    <color>FF0000</color>
                                    <item>
                                        <host>MongoDB</host>
                                        <key>nz.collect.mongo.dhost[{#HOST},delete]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>0040FF</color>
                                    <item>
                                        <host>MongoDB</host>
                                        <key>nz.collect.mongo.dhost[{#HOST},insert]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>2</sortorder>
                                    <color>66BB6A</color>
                                    <item>
                                        <host>MongoDB</host>
                                        <key>nz.collect.mongo.dhost[{#HOST},query]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>3</sortorder>
                                    <color>BF00FF</color>
                                    <item>
                                        <host>MongoDB</host>
                                        <key>nz.collect.mongo.dhost[{#HOST},update]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                    <master_item>
                        <key>nz.collect.mongo[10]</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#HOST}</lld_macro>
                            <path>$.host</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#CONNECTION}</lld_macro>
                            <path>$.connection</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#DELETE}</lld_macro>
                            <path>$.delete</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#INSERT}</lld_macro>
                            <path>$.insert</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#QUERY}</lld_macro>
                            <path>$.query</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#UPDATE}</lld_macro>
                            <path>$.update</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.stats</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
        </template>
        <template>
