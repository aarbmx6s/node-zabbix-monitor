                            <path>$.pm_id</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#MEMORY}</lld_macro>
                            <path>$.memory</path>
                        </lld_macro_path>
                    </lld_macro_paths>
